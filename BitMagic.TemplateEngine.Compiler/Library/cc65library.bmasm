library BitMagic.Cc65.Library.Manager;
using Ca65LibParser;

include "cc65LibParser.cs";

public static Cc65Lib Parse(string filename, string scope)
{
    return Cc65LibParser.Parse(filename, scope);
}

public static void Exports(Cc65Lib lib)
{
    foreach(var i in lib.GenerateExportsStrings())
    {
        @i
    }
}

public static void Code(string segmentName, Cc65Lib lib)
{
    foreach(var i in lib.GenerateCodeStrings(segmentName))
    {
        @i
    }
}